{
  "name": "project-1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:server:dev": "npm --prefix ./server run server:dev",
    "start:server:dev:debug": "npm --prefix ./server run server:dev:debug",
    "watch:server:dev": "nodemon -w ./server/**/*.js --exec \"npm run start:server:dev\"",
    "watch:server:dev:debug": "nodemon -w ./server/**/*.js --exec \"npm run start:server:dev:debug\"",
    "start:server:staging": "npm run start:client:build && npm --prefix ./server run server:staging",
    "server:dev:debug": "npx cross-env DEBUG=* node -r babel-register -r dotenv/config ./bin/www.js dotenv_config_path='../.env'",
    "server:dev": "node -r babel-register -r dotenv/config ./bin/www.js dotenv_config_path='../.env'",
    "server:staging": "node -r babel-register -r dotenv/config ./bin/www.js dotenv_config_path='../.env'",
    "server:prod": "rm -rf build/server && npx babel -d ./build/server ./server -s && node -r dotenv/config ./build/server/bin/www.js dotenv_config_path='.env'",
    "dev": "npx concurrently \"npm run watch:server:dev\" \"npm run watch:client:dev\"",
    "watch:client:dev": "nodemon -w ./client/**/*.js --exec \"npm run start:client:dev\"",
    "start:client:dev": "npm --prefix ./client run client:dev",
    "start:client:build": "npm --prefix ./client run client:build",
    "start:client:test": "npm --prefix ./client run client:test",
    "client:dev": "npx react-scripts start",
    "client:build": "npx react-scripts build",
    "client:test": "npx react-scripts test --env=jsdom"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "mongoose": "^5.0.8",
    "morgan": "~1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "debug": "^3.1.0",
    "nodemon": "^1.17.1"
  },
  "engines": {
    "node": "^8",
    "npm": "^5",
    "npx": "^9",
    "concurrently": "^3"
  }
}
