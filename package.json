{
  "name": "project-1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:server:dev": "npm --prefix ./server run server:dev",
    "watch:server:dev": "nodemon -w ./server/**/*.js --exec \"npm run start:server:dev\"",
    "start:server:staging": "npm run start:client:build && npm --prefix ./server run server:staging",
    "server:dev": "npx cross-env DEBUG=* npx babel-node ./bin/www --presets env,stage-2",
    "server:staging": "npx babel-node ./bin/www --presets env,stage-2",
    "dev": "npx concurrently \"npm run watch:server:dev\" \"npm run watch:client:dev\"",
    "watch:client:dev": "nodemon -w ./client/**/*.js --exec \"npm run start:client:dev\"",
    "start:client:dev": "npm --prefix ./client run client:dev",
    "start:client:build": "npm --prefix ./client run client:build",
    "start:client:test": "npm --prefix ./client run client:test",
    "client:dev": "npx react-scripts start",
    "client:build": "npx react-scripts build",
    "client:test": "npx react-scripts test --env=jsdom"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "express": "~4.15.5",
    "morgan": "~1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "debug": "^3.1.0",
    "nodemon": "^1.17.1"
  }
}
